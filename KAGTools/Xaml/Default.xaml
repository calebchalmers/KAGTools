<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:KAGTools.Xaml"
                    xmlns:converters="clr-namespace:KAGTools.Xaml.Converters">

    <!-- Styles -->
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Styles/FilterBarStyle.xaml"/>
        <ResourceDictionary Source="Styles/ButtonComboBoxStyle.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- Converters -->
    <converters:NullableBoolToBoolConverter x:Key="NullableBoolToBoolConverter"/>
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
</ResourceDictionary>